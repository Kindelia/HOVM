@Bool/False = #L(a #L(* a))

@Bool/True = #L(* #L(a a))

@Nat/Succ = #L(a #L(#@(a b) #L(* b)))

@Nat/Zero = #L(* #L(a a))

@Tree/Leaf = #L(a #L(* #L(#@(a b) b)))

@Tree/Node = #L(a #L(b #L(#@(a #@(b c)) #L(* c))))

@main2 = a
 & @num ~ #E(a)

@main = b
  & @xor_tree ~ #@(a #E(b))
  & @mk_tree ~ #@(@num #@(@Bool/True a))

@mk_tree = #L(#@(@mk_tree__C1 #@(@mk_tree__C0 #@(a b))) #L(a b))

@mk_tree__C0 = #L(a b)
  & @Tree/Leaf ~ #@(a b)

@mk_tree__C1 = #L(#d(a c) #L(* e))
  & @Tree/Node ~ #@(b #@(d e))
  & @mk_tree ~ #@(a #@(@Bool/True b))
  & @mk_tree ~ #@(c #@(@Bool/False d))

@num = m
  & @Nat/Succ ~ #@(l m)
  & @Nat/Succ ~ #@(k l)
  & @Nat/Succ ~ #@(j k)
  & @Nat/Succ ~ #@(i j)
  & @Nat/Succ ~ #@(h i)
  & @Nat/Succ ~ #@(g h)
  & @Nat/Succ ~ #@(f g)
  & @Nat/Succ ~ #@(e f)
  & @Nat/Succ ~ #@(d e)
  & @Nat/Succ ~ #@(c d)
  & @Nat/Succ ~ #@(b c)
  & @Nat/Succ ~ #@(a b)
  & @Nat/Succ ~ #@(@Nat/Zero a)

@xor = #L(#@(#L(a a) #@(@xor__C0 b)) b)

@xor__C0 = #L(#@(@Bool/True #@(@Bool/False a)) a)

@xor_tree = #L(#@(@xor_tree__C0 #@(#L(a a) b)) b)

@xor_tree__C0 = #L(a #L(c e))
  & @xor ~ #@(b #@(d e))
  & @xor_tree ~ #@(a b)
  & @xor_tree ~ #@(c d)
